    <sql>SELECT 
    cc.Crm_Cd_FK, 
    c.DR_NO, 
    c.DATE_OCC, 
    SEC_TO_TIME(FLOOR(TIME_TO_SEC(c.TIME_OCC) / 60) * 60) AS TIME_OCC_Corrigido, 
    c.Status_FK, 
    cl.Location_ID_FK, 
    c.Weapon_Used_Cd_FK, 
    c.Location_Type_Cd_FK
FROM 
    DB_CRIMES_LA.CRIME_CODE AS cc
JOIN 
    DB_CRIMES_LA.CRIME AS c ON cc.DR_NO_FK = c.DR_NO
LEFT JOIN 
    DB_CRIMES_LA.CRIME_LOCATION AS cl ON c.DR_NO = cl.DR_NO_FK
WHERE 
    c.DATE_OCC IS NOT NULL 
